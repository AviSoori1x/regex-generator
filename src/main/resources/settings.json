{
	"recognizers": [
		{
			"name": "numbers",
			"regex": "[0-9]+",
			"description": "Natural numbers"
		},
		{
			"name": "date",
			"regex": "[0-9]{4}-[0-9]{2}-[0-9]{2}",
			"description": "a date"
		},
		{
			"name": "time",
			"regex": "[0-9]{2}:[0-9]{2}:[0-9]{2}(\\.[0-9]{1,3})?",
			"description": "The time"
		},
		{
			"name": "loglevel",
			"regex": "(TRACE|DEBUG|INFO|NOTICE|WARN|ERROR|SEVERE|FATAL)",
			"description": "Level a logger may use"
		}
	]
}